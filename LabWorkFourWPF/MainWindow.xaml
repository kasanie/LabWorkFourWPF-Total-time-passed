<Window x:Class="LabWorkFourWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWorkFourWPF"
        mc:Ignorable="d"
        Title="How much time has passed?" Height="250" Width="500">
    <Grid>
        <ComboBox x:Name="Year" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Year_SelectionChanged"/>
        <ComboBox x:Name="Month" HorizontalAlignment="Left" Margin="190,41,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Month_SelectionChanged"/>
        <ComboBox x:Name="Day" HorizontalAlignment="Left" Margin="362,41,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Day_SelectionChanged"/>
        <Label Content="Год:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120" FontSize="14"/>
        <Label Content="Месяц:" HorizontalAlignment="Left" Margin="190,10,0,0" VerticalAlignment="Top" Width="120" FontSize="14"/>
        <Label Content="Число:" HorizontalAlignment="Left" Margin="362,10,0,0" VerticalAlignment="Top" Width="120" FontSize="14"/>
    </Grid>
</Window>
